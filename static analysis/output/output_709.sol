pragma solidity ^0.8.9;
library SafeMath {
    function addition(uint a, uint b) internal pure returns (uint) {
        return a + b;
    }


    function subtraction(uint a, uint b) internal pure returns (uint) {
        return a - b;
    }


    function multiply(uint a, uint b) internal pure returns (uint) {
        return a * b;
    }


    

    

}

